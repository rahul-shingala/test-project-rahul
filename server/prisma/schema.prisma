datasource postgres {
  provider = "postgresql"
  url      = env("POSTGRESQL_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  createdAt      Boolean
  firstName      DateTime?
  id             String    @id @default(cuid())
  lastName       String?
  password       String
  roles          String[]
  updatedAtfghfd DateTime  @updatedAt
  username       String    @unique
}